#include <bits/stdc++.h>
using namespace std;
int main() {
        int n;
        cin >> n;
        long long arr[n - 1];
        long long sum = 0;
        long long totalsum = (1LL*n * (n + 1)) / 2;
        for (int i = 0; i < n - 1; i++)
        {
                cin >> arr[i];
                sum += arr[i];
        }
        cout << totalsum - sum;
}
